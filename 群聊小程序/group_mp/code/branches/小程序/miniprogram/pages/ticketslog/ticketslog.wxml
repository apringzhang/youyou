<!--tradeAccount.wxml 收入 交易记录-->

<view class="in">
    <view class="left">
        <text class="left-in">充值记录</text>
    </view>
</view>

<view class="record" wx:for="{{list}}" wx:key="this">
    <view class="on">
       <text class="action">充值金额</text>
       <text class="money">微信支付：¥{{item.price}}</text>
    </view>
  <view class="under">
       <text class="time">交易时间: <text class="detail">{{item.create_time}}</text> </text>
       <text class="succerr">交易成功</text>
  </view>
</view>


